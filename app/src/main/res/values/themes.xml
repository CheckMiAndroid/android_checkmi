<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.CheckMi" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="table_color" parent="ThemeOverlay.AppCompat">
        <item name="colorAccent">@color/table_color</item>
    </style>

    <style name="basic_button" parent="Widget.MaterialComponents.Button">
        <item name="backgroundTint">@null</item>
        <item name="background">@drawable/ic_new_seat_3</item>
    </style>
    <style name="green_button" parent="Widget.MaterialComponents.Button">
        <item name="backgroundTint">#112233</item>
    </style>

    <style name="style_seat3" >

        <item name="android:background">@drawable/ic_new_seat_3</item>
