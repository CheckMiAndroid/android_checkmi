<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="view.MainActivity"
    android:background="#FFFFFF">

    <!--//-->
    <!--System UI 인지 로고 인지 그 위에 무언가-->
    <ImageView
        android:id="@+id/Back_green"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:background="@drawable/system_ui"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/Check_Mi"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_gravity="center"
        android:background="@drawable/logo"
        app:layout_constraintBottom_toBottomOf="@+id/Back_green"
        app:layout_constraintEnd_toEndOf="@+id/Back_green"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/Back_green"
        tools:ignore="MissingConstraints" />

    <!--
    <ImageView
        android:id="@+id/menu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/menu"
        app:layout_constraintEnd_toEndOf="@+id/Back_green"
        app:layout_constraintStart_toEndOf="@+id/Check_Mi"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="35dp" />
        -->

    <!--//-->
    <!--자리 이외 View-->
    <LinearLayout
        android:id="@+id/View"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/View1_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/view_1"
            android:rotation="0" />

        <ImageView
            android:id="@+id/View_2_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:background="@drawable/view_2" />

        <ImageView
            android:id="@+id/View_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:background="@drawable/view_3" />

        <ImageView
            android:id="@+id/View_2_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:background="@drawable/view_2" />

        <ImageView
            android:id="@+id/View1_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="17dp"
            android:background="@drawable/view_1"
            android:rotation="270" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/VIew"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/SEat"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Back_green"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/View4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/view_4" />

        <ImageView
            android:id="@+id/View5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="55dp"
            android:background="@drawable/view_5" />

    </LinearLayout>


    <!--//-->
    <!--자리 버튼-->
    <!--창가 자리-->

    <ImageView
        android:id="@+id/View7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/view_7"
        app:layout_constraintBottom_toBottomOf="@id/logo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SEAT"
        tools:ignore="MissingConstraints" />

    <LinearLayout
        android:id="@+id/Seat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/Seat14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_new_seat_41"
            app:tint="#DADADA"/>

        <ImageView
            android:id="@+id/Seat15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_new_seat_4"
            app:tint="#DADADA"/>

        <ImageView
            android:id="@+id/Seat16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:src="@drawable/ic_new_seat_41"
            app:tint="#DADADA"/>

        <ImageView
            android:id="@+id/Seat17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_new_seat_4"
            app:tint="#DADADA"/>

        <ImageView
            android:id="@+id/View6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/view_6" />

        <ImageView
            android:id="@+id/Seat18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_new_seat_2"
            app:tint="#DADADA"/>

        <ImageView
            android:id="@+id/Seat19"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:src="@drawable/ic_new_seat_2"
            app:tint="#DADADA"/>

    </LinearLayout>
    <!--서버 자리-->
    <LinearLayout
        android:id="@+id/SEat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/SEAT"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/Seat0"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_new_seat_3"
            />

        <ImageView
            android:id="@+id/Seat1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="7dp"
            android:src="@drawable/ic_new_seat_3"
            app:tint="#DADADA"/>

        <ImageView
            android:id="@+id/Seat2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="7dp"
            android:src="@drawable/ic_new_seat_3"
            app:tint="#DADADA"/>

        <ImageView
            android:id="@+id/Seat3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="7dp"
            android:src="@drawable/ic_new_seat_3"
            app:layout_constraintEnd_toEndOf="parent"
            app:tint="#DADADA"/>

        <ImageView
            android:id="@+id/Seat4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="7dp"
            android:src="@drawable/ic_new_seat_3"
            app:tint="#DADADA"/>


    </LinearLayout>
    <!--안드? 자리-->
    <LinearLayout
        android:id="@+id/SEAt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/Seat21"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/Seat5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_new_seat_41"
            />

        <ImageView
            android:id="@+id/Seat6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_new_seat_4"
            />

        <ImageView
            android:id="@+id/Seat7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:src="@drawable/ic_new_seat_41"
            />

        <ImageView
            android:id="@+id/Seat8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_new_seat_4"
            />

    </LinearLayout>
    <!--웹 자리-->
    <LinearLayout
        android:id="@+id/SEAT"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/SEat"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/Seat13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_new_seat_3" />

        <ImageView
            android:id="@+id/Seat12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="7dp"
            android:src="@drawable/ic_new_seat_3" />

        <ImageView
            android:id="@+id/Seat11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="7dp"
            android:src="@drawable/seat_5" />

        <ImageView
            android:id="@+id/Seat10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="7dp"
            android:src="@drawable/seat_5" />

        <ImageView
            android:id="@+id/Seat9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="7dp"
            android:src="@drawable/seat_5" />

    </LinearLayout>
    <!--중간 원 자리-->
    <ImageView
        android:id="@+id/Seat21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/seat_6"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />
    <!--김건호 선배님 자리-->

    <!--//-->
    <!--두카미 아래 로고 -->

    <ImageView
        android:id="@+id/Seat20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:src="@drawable/ic_new_seat_7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.774"
        app:layout_constraintStart_toEndOf="@+id/View7"
        app:layout_constraintTop_toBottomOf="@+id/Seat"
        app:tint="#DADADA"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/logo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:background="@drawable/ducami_logo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/View7"
        tools:ignore="MissingConstraints" />



</androidx.constraintlayout.widget.ConstraintLayout>
