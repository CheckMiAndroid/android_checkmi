<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="view.MainActivity"
    android:background="#FFFFFF">

    <!--//-->
    <!--System UI 인지 로고 인지 그 위에 무언가-->
    <ImageView
        android:id="@+id/Back_green"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:background="@drawable/system_ui"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/Check_Mi"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_gravity="center"
        android:background="@drawable/logo"
        app:layout_constraintBottom_toBottomOf="@+id/Back_green"
        app:layout_constraintEnd_toEndOf="@+id/Back_green"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/Back_green"
        tools:ignore="MissingConstraints" />

    <!--
    <ImageView
        android:id="@+id/menu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/menu"
        app:layout_constraintEnd_toEndOf="@+id/Back_green"
        app:layout_constraintStart_toEndOf="@+id/Check_Mi"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="35dp" />
        -->

    <!--//-->
    <!--자리 이외 View-->
    <LinearLayout
        android:id="@+id/View"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/View1_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/view_1"
            android:rotation="0" />

        <ImageView
            android:id="@+id/View_2_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:background="@drawable/view_2" />

        <ImageView
            android:id="@+id/View_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:background="@drawable/view_3" />

        <ImageView
            android:id="@+id/View_2_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:background="@drawable/view_2" />

        <ImageView
            android:id="@+id/View1_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="17dp"
            android:background="@drawable/view_1"
            android:rotation="270" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/VIew"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/first_line_seat"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Back_green"
        tools:ignore="MissingConstraints">

        <ImageView
            android:id="@+id/View4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/view_4" />

        <ImageView
            android:id="@+id/View5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="55dp"
            android:background="@drawable/view_5" />

    </LinearLayout>


    <!--//-->
    <!--자리 버튼-->
    <!--창가 자리-->

    <ImageView
        android:id="@+id/View7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/view_7"
        app:layout_constraintBottom_toBottomOf="@id/logo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/third_line_seat"
        tools:ignore="MissingConstraints" />

    <LinearLayout
        android:id="@+id/fourth_line_seat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <androidx.appcompat.widget.AppCompatButton

            android:id="@+id/Seat14"
            style="@style/style_seat41"

            />

        <androidx.appcompat.widget.AppCompatButton

            android:id="@+id/Seat15"
            style="@style/style_seat4"

            />

        <androidx.appcompat.widget.AppCompatButton

            android:layout_marginTop="10dp"
            android:id="@+id/Seat16"
            style="@style/style_seat41"

            />

        <androidx.appcompat.widget.AppCompatButton

            android:id="@+id/Seat17"
            style="@style/style_seat4"
            />

        <ImageView
            android:id="@+id/View6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="30dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/view_6"
            android:gravity="center"
            />

        <androidx.appcompat.widget.AppCompatButton
            android:button="@null"
            android:id="@+id/Seat18"
            android:layout_width="30dp"
            android:layout_height="55dp"
            android:background="@drawable/ic_new_seat_2"
            android:gravity="center"

            />


        <androidx.appcompat.widget.AppCompatButton
            android:button="@null"
            android:id="@+id/Seat19"
            android:layout_width="30dp"
            android:layout_height="55dp"
            android:layout_marginTop="5dp"
            android:background="@drawable/ic_new_seat_2"
            android:gravity="center"
            />

    </LinearLayout>
    <!--서버 자리-->


    <LinearLayout

        android:id="@+id/first_line_seat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintBottom_toTopOf="@+id/third_line_seat"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        >

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/Seat0"
            style="@style/style_seat3"
            />

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/Seat1"
            android:layout_marginLeft="7dp"
            style="@style/style_seat3"
             />

        <androidx.appcompat.widget.AppCompatButton

            android:id="@+id/Seat2"
            android:layout_marginLeft="7dp"
            style="@style/style_seat3"
            />

        <androidx.appcompat.widget.AppCompatButton
            style="@style/style_seat3"
            android:id="@+id/Seat3"
            android:layout_marginLeft="7dp"

            />

        <androidx.appcompat.widget.AppCompatButton

            android:id="@+id/Seat4"
            android:layout_marginLeft="7dp"
            style="@style/style_seat3"
            />

</LinearLayout>






    <!--안드? 자리-->
    <LinearLayout
        android:id="@+id/second_line_seat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/Seat21"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints"
        android:layout_marginEnd="20dp"
        >

        <androidx.appcompat.widget.AppCompatButton

            android:id="@+id/Seat5"

            style="@style/style_seat41"

            />

        <androidx.appcompat.widget.AppCompatButton

            android:id="@+id/Seat6"
            style="@style/style_seat4"

            />

        <androidx.appcompat.widget.AppCompatButton

            android:layout_marginTop="10dp"
            android:id="@+id/Seat7"
            style="@style/style_seat41"
            />

        <androidx.appcompat.widget.AppCompatButton

            android:id="@+id/Seat8"
            style="@style/style_seat4"
            />

    </LinearLayout>
    <!--웹 자리-->
    <LinearLayout
        android:id="@+id/third_line_seat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/first_line_seat"
        tools:ignore="MissingConstraints">

        <androidx.appcompat.widget.AppCompatButton

            android:id="@+id/Seat13"
            style="@style/style_seat3"
            />


        <androidx.appcompat.widget.AppCompatButton

            android:id="@+id/Seat12"

            android:layout_marginLeft="7dp"
            style="@style/style_seat3"
            />

        <androidx.appcompat.widget.AppCompatButton

            android:id="@+id/Seat11"

            android:layout_marginLeft="7dp"
            style="@style/style_seat3"
            />

        <androidx.appcompat.widget.AppCompatButton

            android:id="@+id/Seat10"

            android:layout_marginLeft="7dp"
            style="@style/style_seat3"
            />

        <androidx.appcompat.widget.AppCompatButton

            android:id="@+id/Seat9"

            android:layout_marginLeft="7dp"
            style="@style/style_seat3"
            />

    </LinearLayout>
    <!--중간 원 자리-->
    <!--김건호 선배님 자리-->

    <!--//-->
    <!--두카미 아래 로고 -->

    <CheckBox
        android:id="@+id/Seat21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/circle_mark"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:button="@null"
        tools:ignore="MissingConstraints" />

    <androidx.appcompat.widget.AppCompatButton
        android:button="@null"
        android:id="@+id/Seat20"
        android:layout_width="82dp"
        android:layout_height="38dp"
        android:layout_marginTop="4dp"
        android:background="@drawable/ic_new_seat_7"
        android:gravity="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toEndOf="@+id/View7"
        app:layout_constraintTop_toBottomOf="@+id/fourth_line_seat"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/logo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:background="@drawable/ducami_logo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/View7"
        tools:ignore="MissingConstraints" />




</androidx.constraintlayout.widget.ConstraintLayout>
